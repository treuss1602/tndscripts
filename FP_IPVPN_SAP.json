      "special": false,
      "dynamicallyMapped": false,
      "modifyOperation": "ModifyParameter",
      "exampleValue": "10.10.24.128/28-10.20.0.1-\"VRF1\"-\"22\"-2",
      "cramerStorage": "IPVPN_SAP_GE",
